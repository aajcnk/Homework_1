<h1>Войти</h1>

<%= form_tag sessins_path do %>
  <div class="field">
    <%= label_tag :email %> <br />
    <%= text_field_tag :email, params[:email] %> 
  </div>

  <div class="field">
    <%= label_tag :password %> <br />
    <%= text_field_tag :password %> 
  </div>
 
  <div class="actions"><%= submit_tag "Войти" %>
  </div>
<% end %>